{
  "hosting": {
    "public": ".output/public",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "redirects": [
      {
        "source": "/",
        "destination": "https://vps.incomexsaigoncorp.vn/",
        "type": 301
      },
      {
        "source": "/:path*",
        "destination": "https://vps.incomexsaigoncorp.vn/:path",
        "type": 301
      }
    ]
  }
}
